{# a *-to-many collection of values #}
{% if value is iterable %}
    {% if value|length > 0 %}
        {% spaceless %}
            {% for item in value %}{{ item|raw }}{% if not loop.last %}{{ field_options.type_options.separator is defined ? field_options.type_options.separator : ', ' }}{% endif %}{% endfor %}
        {% endspaceless %}
    {% endif %}
    {# a simple *-to-one value associated with an entity managed by this backend #}
{% else %}
    {{ value }}
{% endif %}
